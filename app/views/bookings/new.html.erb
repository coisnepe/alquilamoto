<h1>Bookings Procedure</h1>

<div class="container">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3">
      <%= simple_form_for [@bike, @booking] do |f| %>
        <%= f.input :date_begin %>
        <%= f.input :date_end %>
        <%= f.button :submit, class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>
